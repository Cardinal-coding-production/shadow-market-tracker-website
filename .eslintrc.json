{
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true
  },
  "globals": {
    "chrome": "readonly",
    "importScripts": "readonly",
    "self": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}

